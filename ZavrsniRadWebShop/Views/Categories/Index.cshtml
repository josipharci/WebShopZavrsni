@model IEnumerable<ZavrsniRadWebShop.Models.Category>

<div class="container mt-5">
    <h1 class="mb-4">Categories</h1>
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var category in Model)
            {
                <tr>
                    <td>@category.Name</td>
                </tr>
            }
        </tbody>
    </table>
</div>